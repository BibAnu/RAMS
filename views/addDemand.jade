form.form-horizontal(ng-submit='addDemand()')
    .form-group
        label.col-sm-4.control-label
            h3 Add Demand
        .col-sm-8
    .form-group
        label.col-sm-2.control-label Visa No
        .col-sm-10
            input.form-control(type='number', placeholder='Visa No.', ng-model='newDemand.visano' required)
    .form-group
        label.col-sm-2.control-label Company
        .col-sm-10
            select.form-control(ng-model='newDemand.name')
                option(ng-repeat="item in items" value="{{item.name}}") {{item.name}}
    .form-group
        .col-sm-offset-2.col-sm-10
            button.btn.btn-success(type='submit') Submit
            button.btn.btn-danger(ui-sref="dashboard.demands") Cancel
